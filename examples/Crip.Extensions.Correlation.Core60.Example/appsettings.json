{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Crip.Extensions.Correlation.Core60.Example.Controllers": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o} [{Level:u3}] {Message}{NewLine}{Properties}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "ApplicationName"
    ],
    "Properties": {
      "Application": "Net6.0-Example"
    }
  },
  "AllowedHosts": "*",
  "TestController": {
    "BaseUrl": "https://localhost:7027/"
  }
}
